{
  "name": "granax",
  "version": "3.1.3",
  "description": "complete client implementation of the tor control protocol",
  "main": "index.js",
  "scripts": {
    "postinstall": "node script/download-tbb.js",
    "test": "npm run test-suite && npm run linter",
    "test-suite": "mocha test/** --recursive",
    "coverage": "istanbul cover _mocha -- --recursive",
    "linter": "eslint index.js lib test",
    "generate-docs": "mkdir -p ./public && rm -r ./public && jsdoc index.js lib -r -R README.md -u ./doc -c .jsdoc.json --verbose -d ./public"
  },
  "engines": {
    "node": "^8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/bookchin/granax.git"
  },
  "keywords": [
    "tor",
    "onion",
    "control",
    "protocol",
    "hidden",
    "service",
    "anonymous",
    "dark",
    "net",
    "deep",
    "web",
    "stem"
  ],
  "author": "Gordon Hall <gordonh@member.fsf.org>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://gitlab.com/bookchin/granax/issues"
  },
  "homepage": "https://gitlab.com/bookchin/granax#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint": "^3.19.0",
    "ink-docstrap": "git+https://gitlab.com/bookchin/docstrap.git",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0"
  },
  "dependencies": {
    "async": "^2.3.0",
    "latest-torbrowser-version": "^2.0.1",
    "merge": "^1.2.0",
    "mkdirp": "^0.5.1"
  },
  "optionalDependencies": {
    "7zip": "0.0.6",
    "ncp": "^2.0.0"
  }
}
